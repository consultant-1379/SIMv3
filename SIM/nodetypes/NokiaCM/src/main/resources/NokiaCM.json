{
  "name": "NokiaCM",
  "internal": true,
  "baseType": "SftpNode",
  "properties": [
    {
      "name": "sftpKeyFile",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": ""
      }
    },
    {
      "name": "remoteDirectory",
      "required": true,
      "type": {
        "type": "string",
        "defaultString": "/d/oss/global/var/racops/export/"
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "^OM1SK_[\\w]+_CMData_Export.xml.gz",
        "regex": true
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata/eniq_oss_1/dim_n_raml/dir1/"
      }
    },
    {
      "name": "cronBasedRop",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "50 2 * * *"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    },
    {
      "name": "ignoreHistory",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "true"
      }
    }
  ]
}