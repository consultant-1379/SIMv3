{
  "name": "NokiaPM",
  "internal": true,
  "baseType": "SftpNode",
  "properties": [
    {
      "name": "sftpKeyFile",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": ""
      }
    },
    {
      "name": "remoteDirectory",
      "required": true,
      "type": {
        "type": "string",
        "defaultString": "/d/oss/global/var/mediation/north/pm/export/"
      }
    },
    {
      "name": "parentDirPattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "^[0-9]{10}$",
        "regex": true
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "^etlexpmx_[\\w]+_[0-9]+_[0-9]+.xml\\.gz",
        "regex": true
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata/eniq_oss_1/dc_n_xml/dir1/"
      }
    },
    {
      "name": "currentHourLocalDir",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "true"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    }
  ]
}