{
  "name": "HuaweiPM",
  "internal": true,
  "baseType": "SftpNode",
  "properties": [
    {
      "name": "remoteDirectory",
      "required": true,
      "type": {
        "type": "string",
        "defaultString": "/export/home/sysm/opt/oss/server/var/fileint/pmneexport/neexport_*"
      }
    },
    {
      "name": "parentDirPattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": ".*",
        "regex": true
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "^A.*.xml.gz$",
        "regex": true
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata/eniq_oss_1/pm_som/1/"
      }
    }
  ]
}