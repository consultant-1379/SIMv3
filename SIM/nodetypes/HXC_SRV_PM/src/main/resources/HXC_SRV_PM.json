      "required": false,
      "type": {
        "type": "string",
        "defaultString": ""
      }
    },
    {
      "name": "remoteDirectory",
      "required": true,
      "type": {
        "type": "string",
        "defaultString": "/opt/hxc/stats/eniq/"
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "^(?!.*total).*.xls",
        "regex": true
      }
    },
    {
      "name": "renamePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "%IP_ADDRESS%-%HASH_CODE%_service-%R_FILE_NAME%",
        "regex": false
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata_sim/eniq_oss_1/dc_cc_hxc/HXC/"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    }
  ]
}