{
  "name": "AIR_PM",
  "internal": true,
  "baseType": "CsvAppender",
  "properties": [
    {
      "name": "remoteDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/var/opt/fds/statistics/"
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": ".*AUV.*stat|.*FSC-A.*stat|.*FSC-B.*stat|.*FSC-M.*stat|.*AirIp.*stat|.*MapInterface.*stat|.*Notification.*stat|.*MapInterfaceHD.*stat",
        "regex": true
      }
    },
    {
      "name": "renamePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "%IP_ADDRESS%-%HASH_CODE%_%R_FILE_NAME%",
        "regex": false
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata_sim/eniq_oss_1/dc_e_cs/AIR/"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    },
    {
      "name": "csvDateHeaderIndex",
      "required": false,
      "type": {
        "type": "integer",
        "defaultValue": 0
      }
    },
    {
      "name": "csvDateHeaderFormat",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "yyyy-MM-dd-HHmm",
        "datetime": true
      }
    },
    {
      "name": "csvRopStartPattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "yyyyMMdd-HHmm",
        "datetime": true
      }
    }
  ]
}