      "required": false,
      "type": {
        "type": "string",
        "defaultString": ""
      }
    },
    {
      "name": "remoteDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/var/log/esa/pm3gppXml/"
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": ".*StatisticsPMJob_ESA.*xml",
        "regex": true
      }
    },
    {
      "name": "renamePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "%IP_ADDRESS%-%HASH_CODE%_%R_FILE_NAME%",
        "regex": false
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata_sim/eniq_oss_1/dc_e_cs_vs_xml/VS/"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    }
  ]
}