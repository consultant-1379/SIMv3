{
  "name": "OCC_MET_PM",
  "internal": true,
  "baseType": "CsvAppender",
  "properties": [
    {
      "name": "remoteDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/opt/occ/var/performance/"
      }
    },
    {
      "name": "namePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "meters_.*[0-9]\\.log",
        "regex": true
      }
    },
    {
      "name": "renamePattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "%IP_ADDRESS%-%HASH_CODE%_%R_FILE_NAME%",
        "regex": false
      }
    },
    {
      "name": "destinationDirectory",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "/eniq/data/pmdata_sim/eniq_oss_1/dc_e_occ_meters/OCCMeters/"
      }
    },
    {
      "name": "localCleanup",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    },
    {
      "name": "csvDateHeaderIndex",
      "required": false,
      "type": {
        "type": "integer",
        "defaultValue": 0
      }
    },
    {
      "name": "csvDateHeaderFormat",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "yyyy-MM-dd HH:mm:ss",
        "datetime": true
      }
    },
    {
      "name": "csvRopStartPattern",
      "required": false,
      "type": {
        "type": "string",
        "defaultString": "yyyyMMdd-HHmm",
        "datetime": true
      }
    },
    {
      "name": "hasHeader",
      "required": false,
      "type": {
        "type": "boolean",
        "defaultString": "false"
      }
    }
  ]
}